{
	"name": "image-shrinker",
	"productName": "ImageShrinker",
	"version": "1.0.0",
	"description": "App to shrink the image size",
	"main": "main.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "electron .",
		"dev": "nodemon --exec npm start",
		"package-linux": "electron-packager . image-shrinker --overwrite --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true",
		"create-debian-installer": "electron-installer-debian --src image-shrinker-linux-x64/ --arch amd64 --config debian.json"
	},
	"keywords": [
		"nil1729",
		"electron"
	],
	"author": "Nilanjan Deb",
	"license": "MIT",
	"devDependencies": {
		"electron": "^13.1.2",
		"electron-installer-debian": "^3.1.0",
		"electron-packager": "^15.2.0",
		"nodemon": "^2.0.7"
	},
	"dependencies": {
		"byte-size": "^7.0.1",
		"electron-log": "^4.3.5",
		"imagemin": "^7.0.1",
		"imagemin-mozjpeg": "^8.0.0",
		"imagemin-pngquant": "^8.0.0",
		"slash": "^3.0.0"
	}
}
